{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Rough Draft",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#fffff1",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#393532",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#4e759d",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#584f46",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#4d4741",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"custom": {
                        "lineHeight": {
                                "xxxxx-large": "clamp( 2rem, calc( 2rem + ( ( 1vw - 0.4rem ) * 5.2083 ) ), 3.25rem )",
                                "xxxx-large": "clamp( 2rem, calc( 2rem + ( ( 1vw - 0.4rem ) * 2.2137 ) ), 2.5313rem )",
                                "xxx-large": "clamp( 2.25rem, calc( 2.25rem + ( ( 1vw - 0.4rem ) * -2.0833 ) ), 1.75rem )",
                                "xx-large": "clamp( 2rem, calc( 2rem + ( ( 1vw - 0.4rem ) * -1.0417 ) ), 1.75rem )",
                                "x-large": "1.75rem",
                                "large": "1.75",
                                "medium": "1.5rem",
                                "small": "1.5rem",
                                "x-small": "1.25rem"
                        }
                },
                "layout": {
                        "contentSize": "38rem",
                        "wideSize": "64rem"
                },
		"spacing": {
			"customSpacingSize": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "1px",
					"slug": "px",
					"name": "Pixel - Fixed (1px)"
				},
				{
					"size": "0.25rem",
					"slug": "10",
					"name": "Step 1 - Fixed (0.25rem)"
				},
				{
					"size": "0.5rem",
					"slug": "20",
					"name": "Step 2 - Fixed (0.5rem)"
				},
				{
					"size": "0.75rem",
					"slug": "30",
					"name": "Step 3 - Fixed (0.75rem)"
				},
				{
					"size": "1rem",
					"slug": "40",
					"name": "Step 4 - Fixed (1rem)"
				},
				{
					"size": "1.25rem",
					"slug": "50",
					"name": "Step 5 - Fixed (1.25rem)"
				},
				{
					"size": "1.5rem",
					"slug": "60",
					"name": "Step 6 - Fixed (1.5rem)"
				},
				{
					"size": "1.75rem",
					"slug": "70",
					"name": "Step 7 - Fixed (1.75rem)"
				},
				{
					"size": "2rem",
					"slug": "80",
					"name": "Step 8 - Fixed (2rem)"
				},
				{
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 1.0417)), 2.25rem)",
					"slug": "90",
					"name": "Step 9 - Fluid (2.25rem)"
				},
				{
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 2.0833)), 2.5rem)",
					"slug": "100",
					"name": "Step 10 - Fluid (2.5rem)"
				},
				{
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 4.1667)), 3rem)",
					"slug": "120",
					"name": "Step 12 - Fluid (3rem)"
				},
				{
					"size": "clamp(2rem, calc(2rem + ((1vw - 0.4rem) * 8.3333)), 4rem)",
					"slug": "160",
					"name": "Step 16 - Fluid (4rem)"
				},
				{
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 8.3333)), 6rem)",
					"slug": "240",
					"name": "Step 24 - Fluid (6rem)"
				},
				{
					"size": "clamp(4rem, calc(4rem + ((1vw - 0.4rem) * 16.6667)), 8rem)",
					"slug": "320",
					"name": "Step 32 - Fluid (8rem)"
				}
			]
		},
		"typography": {
			"fluid": false,
			"fontSizes": [
				{
                                        "slug": "x-small",
                                        "size": "0.875rem",
                                        "name": "XS"
                                },
                                {
                                        "slug": "small",
                                        "size": "0.9375rem",
                                        "name": "Small"
                                },
                                {
                                        "slug": "medium",
                                        "size": "1rem",
                                        "name": "Medium"
                                },
				{
                                        "slug": "large",
                                        "size": "1.125rem",
                                        "name": "Large"
                                },
                                {
                                        "slug": "x-large",
                                        "size": "clamp(1.1875rem, calc(1.1875rem + ((1vw - 0.4rem) * 0.2604)), 1.25rem)",
                                        "name": "XL"
                                },
                                {
                                        "slug": "xx-large",
                                        "size": "clamp(1.25rem, calc(1.25rem + ((1vw - 0.4rem) * 1.0417)), 1.5rem)",
                                        "name": "2XL"
                                },
                                {
                                        "slug": "xxx-large",
                                        "size": "clamp(1.375rem, calc(1.375rem + ((1vw - 0.4rem) * 2.0833)), 1.875rem)",
                                        "name": "3XL"
                                },
                                {
                                        "slug": "xxxx-large",
                                        "size": "clamp(1.5rem, calc(1.5rem + ((1vw - 0.4rem) * 3.125)), 2.25rem)",
                                        "name": "4XL"
                                },
                                {
                                        "slug": "xxxxx-large",
					"size": "clamp(1.75rem, calc(1.75rem + ((1vw - 0.4rem) * 5.2083)), 3rem)",
                                        "name": "5XL"
                                }
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var( --wp--preset--color--base )",
			"gradient": "linear-gradient( 225deg, #584f4625, transparent 25% ), linear-gradient( 45deg, #584f4625, transparent 25% )",
			"text": "var( --wp--preset--color--contrast )"
		},
                "spacing": {
                        "blockGap": "2rem",
			"padding": {
				"top": "var( --wp--preset--spacing--120 )",
				"bottom": "var( --wp--preset--spacing--120 )",
				"left": "var( --wp--preset--spacing--80 )",
				"right": "var( --wp--preset--spacing--80 )"
			}
                },
		"typography": {
			"fontFamily": "var( --wp--preset--font-family--dm-sans )",
			"fontSize": "var( --wp--preset--font-size--large )",
			"lineHeight": "var( --wp--custom--line-height--large )"
		},
		"blocks": {
			"core/code":{
				"border": {
					"color": "#584f463d",
					"width": "1px",
					"style": "solid",
					"left": {
						"color": "var( --wp--preset--color--secondary )",
						"width": "var( --wp--preset--spacing--10 )"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)",
					"background": "rgb( 88 79 70 / 2.5% )",
					"gradient": "repeating-linear-gradient( #fffff1, #fffff1 2rem, rgb( 88 79 70 / 15% ) calc( 2rem + 1px ), rgb( 88 79 70 / 15% ) calc( 2rem + 1px ) );"
				},
				"spacing": {
					"padding": {
						"top": "0",
						"bottom": "0",
						"right": "var( --wp--preset--spacing--80 )",
						"left": "var( --wp--preset--spacing--70 )"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"fontSize": "1rem",
					"fontWeight": "700",
					"lineHeight": "calc( 2rem + 1px )"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": "var( --wp--preset--spacing--80 )"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--large )",
					"fontWeight": "700",
					"lineHeight": "var( --wp--custom--line-height--large )"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )",
					"lineHeight": "var( --wp--custom--line-height--medium )"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )",
					"lineHeight": "var( --wp--custom--line-height--medium )"
				}
			},
			"core/comment-reply-link": {
				"spacing": {
					"padding": {
						"bottom": "var( --wp--preset--spacing--40 )"
					}
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontStyle": "italic"
				}
			},
			"core/comment-template": {
				"border": {
					"width": "var( --wp--preset--spacing--10 )",
					"style": "solid",
					"color": "var( --wp--preset--color--secondary )"
				},
				"spacing": {
					"padding": "var( --wp--preset--spacing--80 ) var( --wp--preset--spacing--80 ) var( --wp--preset--spacing--40 )"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xxx-large )",
					"lineHeight": "var( --wp--custom--line-height--xxx-large )"
				}
			},
			"core/file": {
				"border": {
					"color": "var( --wp--preset--color--secondary )",
					"width": "var( --wp--preset--spacing--10 )",
					"style": "solid"
				},
				"spacing": {
					"padding": "var( --wp--preset--spacing--80 )"
				}
			},
			"core/gallery": {
				"spacing": {
					"blockGap": "var( --wp--preset--spacing--40 )"
				},
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var( --wp--preset--font-size--small )",
							"lineHeight": "var( --wp--custom--line-height--small )"
						}
					}
				}
			},
			"core/image": {
				"border": {
					"color": "#584f4625",
					"width": "var( --wp--preset--spacing--px )",
					"style": "solid"
				},
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var( --wp--preset--font-size--small )",
							"lineHeight": "var( --wp--custom--line-height--small )"
						}
					}
				}
			},
			"core/navigation": {
				"color": {
					"text": "var( --wp--preset--color--secondary )"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--dm-sans )",
					"fontSize": "var( --wp--preset--font-size--medium )",
					"fontWeight": "400",
					"lineHeight": "1em"
				}
			},
			"core/navigation-link": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )"
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )"
				}
			},
			"core/post-comments-form": {
				"border": {
					"width": "var( --wp--preset--spacing--10 )",
					"style": "solid",
					"color": "var( --wp--preset--color--secondary )"
				},
				"spacing": {
					"margin": "var( --wp--preset--spacing--80 ) 0",
					"padding": "var( --wp--preset--spacing--80 ) var( --wp--preset--spacing--80 ) calc( var( --wp--preset--spacing--80 ) - 1em )"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )",
					"lineHeight": "var( --wp--preset--line-height--medium )"
				}
			},
			"core/post-featured-image": {
				"border": {
					"color": "#584f4625",
					"width": "var( --wp--preset--spacing--px )",
					"style": "solid"
				}
			},
			"core/post": {
				"color": {
					"background": "#000000",
					"text": "#ffffff"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--medium )"
				}
			},
			"core/post-title": {
				"typography": {
					"fontWeight": "900"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var( --wp--preset--color--tertiary )"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "var( --wp--preset--spacing--10 ) 0",
					"style": "solid",
					"color": "var( --wp--preset--color--secondary )"
				},
				"spacing": {
					"padding": "var( --wp--preset--spacing--30 ) 0"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontSize": "var( --wp--preset--font-size--xx-large )",
					"lineHeight": "var( --wp--custom--line-height--xx-large )"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "0.75em"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontSize": "var( --wp--preset--font-size--large )",
					"fontStyle": "italic",
					"lineHeight": "var( --wp--custom--line-height--large )"
				}
			},
			"core/quote": {
				"border": {
					"width": "0 0 0 var( --wp--preset--spacing--10 )",
					"style": "solid",
					"color": "var( --wp--preset--color--secondary )"
				},
				"spacing": {
					"padding": "0 var( --wp--preset--spacing--80 ) 0 var( --wp--preset--spacing--70 )"
				},
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/separator": {
				"border": {
					"width": "calc( var( --wp--preset--spacing--10 ) / 2 )"
				},
				"color": {
					"background": "var( --wp--preset--color--secondary )",
					"text": "var( --wp--preset--color--secondary )"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontSize": "var( --wp--preset--font-size--x-large )",
					"fontStyle": "normal",
					"fontWeight": "900",
					"letterSpacing": "-0.05rem",
					"lineHeight": "var( --wp--custom--line-height--x-large )"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var( --wp--preset--color--tertiary )"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/table": {
				"border": {
					"width": "var( --wp--preset--spacing--px )",
					"style": "solid",
					"color": "var( --wp--preset--color--secondary )"
				}
			}
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var( --wp--preset--color--primary )",
					"text": "var( --wp--preset--color--white )"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontSize": "var( --wp--preset--font-size--medium )",
					"lineHeight": "var( --wp--custom--line-height--medium )"
				},
				":hover": {
					"color": {
						"background": "var( --wp--preset--color--contrast )",
						"text": "var( --wp--preset--color--base )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"color": {
						"background": "var( --wp--preset--color--contrast )",
						"text": "var( --wp--preset--color--base )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":active": {
					"color": {
						"background": "var( --wp--preset--color--primary )",
						"text": "var( --wp--preset--color--base )"
					}
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontSize": "var( --wp--preset--font-size--small )",
					"lineHeight": "var( --wp--custom--line-height--small )"
				}
			},
			"cite": {
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontStyle": "italic"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xxxxx-large )",
					"lineHeight": "var( --wp--custom--line-height--xxxxx-large )"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xxxx-large )",
					"lineHeight": "var( --wp--custom--line-height--xxxx-large )"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xxx-large )",
					"lineHeight": "var( --wp--custom--line-height--xxx-large )"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--xx-large )",
					"lineHeight": "var( --wp--custom--line-height--xx-large )"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--x-large )",
					"lineHeight": "var( --wp--custom--line-height--x-large )"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var( --wp--preset--font-size--large )",
					"fontWeight": "900",
					"lineHeight": "var( --wp--custom--line-height--large )",
					"textTransform": "none"
				}
			},
			"heading": {
				"color": {
					"text": "var( --wp--preset--color--tertiary )"
				},
				"spacing": {
					"margin": "0"
				},
				"typography": {
					"fontFamily": "var( --wp--preset--font-family--ibm-plex-mono )",
					"fontWeight": "900",
					"letterSpacing": "-0.05rem",
					"textTransform": "none"
				}
			},
			"link": {
				"color": {
					"text": "var( --wp--preset--color--primary )"
				},
				":hover": {
					"color": {
						"text": "var( --wp--preset--color--tertiary )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"color": {
						"text": "var( --wp--preset--color--tertiary )"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":active": {
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		}
	}
}
